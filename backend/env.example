# Development Environment
PORT=5000
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=Cluster0
NODE_ENV=development
FRONTEND_URL=http://localhost:5173
BACKEND_URL=http://localhost:5000

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-for-development-2024

# Google OAuth (Required for Google Login)
# Get these from Google Cloud Console > APIs & Services > Credentials
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# SMS Service (Optional - for phone verification)
SMS_API_KEY=your-sms-api-key
SMS_API_SECRET=your-sms-api-secret

# Rabbit Payment Gateway Configuration
RABBIT_API_URL=https://api.pgw.rabbit.co.th
RABBIT_APPLICATION_ID=your-rabbit-application-id
RABBIT_PUBLIC_KEY=your-rabbit-public-key
RABBIT_COMPANY_ID=your-rabbit-company-id
RABBIT_API_KEY=your-rabbit-api-key